		var lc;
		
		%%BLINKDIS
		if(nOM==0) return false;
		ClearTimer("smHnd");
		
		if((om[1].pHS != lmcHS) && om[1].pHS) {
			if(lmcHS) hsHoverSel(1,0,1);
			lmcHS = om[1].pHS;
			SwapMC(mode, om[1].pHS);
		}
		
		if(mode==0) {
			if(om[nOM].sc!=null) HoverSel(1);
			if(nOM>1)
				if(om[nOM-1].sc!=null) {
					om[nOM-1].ssc = om[nOM-1].sc;
